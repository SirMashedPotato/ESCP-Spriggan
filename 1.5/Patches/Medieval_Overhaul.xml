<?xml version="1.0" encoding="utf-8" ?>
<Patch>


	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Medieval Overhaul</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>

				<li Class="PatchOperationReplace">
					<xpath>/Defs/ThingDef[@Name="ESCP_SprigganThingBase"]/race/leatherDef</xpath>
					<value>
						<leatherDef>DankPyon_RawWood</leatherDef>
					</value>
				</li>

				<li Class="PatchOperationAdd">
					<xpath>Defs/MedievalOverhaul.SeperateHideList[defName="WhiteList"]/blackListLeathers</xpath>
					<value>
						<li>DankPyon_RawWood</li>
					</value>
				</li>

			</operations>
		</match>
	</Operation>


</Patch>

